<odoo>

    <record id="efficy_pull_wizard_form" model="ir.ui.view">
        <field name="name">efficy.pull.wizard.form</field>
        <field name="model">efficy.pull.wizard</field>
        <field name="arch" type="xml">
            <form>
                <header>
                    <button name="action_fetch_records" type="object" string="Fetch Records"/>
                </header>
                <group>
                    <field name="record_ids">
                        <tree editable="bottom">
                            <field name="key"/>
                        </tree>
                    </field>
                </group>
                <footer>
                    <button name="action_pull_records" type="object" string="Pull Records"/>
                    <button special="cancel" string="Cancel"/>
                </footer>
            </form>
        </field>
    </record>

</odoo>